$menuText:#000;// 边栏文字颜色
$menuActiveText:#3185fc;// 边栏选中文字颜色
$subMenuActiveText:#222; //边栏子菜单选中文字颜色 // https://github.com/ElemeFE/element/issues/12951

$menuBg:#fff;// 边栏背景色
$menuHover:#eee;// 边栏hover色

$subMenuBg:#fafafa;// 边栏子菜单背景色
$subMenuHover:#eee;// 边栏子菜单hover色

$sideBarWidth: 200px;// 边栏宽度
$selectWidth:100px;// 聚合管理添加流量管理中子分组的选择框宽度
$selectHeight:30px;// 聚合管理添加流量管理中子分组的选择框高低度





// the :export directive is the magic sauce for webpack
// https://www.bluematador.com/blog/how-to-share-variables-between-js-and-sass
:export {
  menuText: $menuText;
  menuActiveText: $menuActiveText;
  subMenuActiveText: $subMenuActiveText;
  menuBg: $menuBg;
  menuHover: $menuHover;
  subMenuBg: $subMenuBg;
  subMenuHover: $subMenuHover;
  sideBarWidth: $sideBarWidth;
  selectWidth:$selectWidth;
}
