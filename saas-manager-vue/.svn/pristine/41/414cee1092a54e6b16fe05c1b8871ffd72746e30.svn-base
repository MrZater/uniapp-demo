import request from '@/utils/request'
// 广告位
export function getPlaceList() {
  return request({
    url: '/place/list',
    methods: 'get'
  })
}

export function getPlacePager(data) {
  return request({
    url: '/place/pager',
    method: 'post',
    data
  })
}

export function addAdplace(data) {
  return request({
    url: '/place/add',
    method: 'post',
    data
  })
}

export function updateAdplace(data) {
  return request({
    url: '/place/update',
    method: 'post',
    data
  })
}

export function getPlaceListByAppId(data) {
  return request({
    url: '/place/listByAppId',
    method: 'post',
    data
  })
}
