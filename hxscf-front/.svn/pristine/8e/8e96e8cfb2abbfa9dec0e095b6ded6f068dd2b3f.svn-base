export default {
  //根据value获取对应的text
  getTextByCode(type, value) {
    if (!value) return "-";
    let stat = codes[type].every((item) => {
      return item["value"] != value;
    });
    if (stat) {
      return "-";
    } else {
      return codes[type].filter((item) => {
        return item["value"] == value;
      })[0]["text"];
    }
  },
  //select下拉框
  getSelectByCode(type) {
    return codes[type];
  },
};
